{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "env": {
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "space-before-function-paren": 0,
    "react/jsx-boolean-value": 0,
    "comma-dangle": 0,
    "no-useless-rename": 0,
    "operator-linebreak": ["error", "before"]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
